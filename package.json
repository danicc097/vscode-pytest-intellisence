{
  	"name": "vscode-pytest",
	"displayName": "Pytest IntelliSense",
	"description": "Autocompletion and Go To Definition support for Pytest fixtures",
	"version": "0.1.1",
	"publisher": "reverbc",
    "author": "Reverb Chu",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/reverbc/vscode-pytest.git"
	},
	"bugs": {
		"url": "https://github.com/reverbc/vscode-pytest/issues",
		"email": "vscode-pytest@reverbc.tw"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"python",
		"pytest"
	],
	"icon": "icon.png",
	"activationEvents": [
        "onLanguage:python"
	],
	"extensionDependencies": [
		"ms-python.python"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"typescript": "^4.1.3",
		"vsce": "1.87.0"
	}
}
